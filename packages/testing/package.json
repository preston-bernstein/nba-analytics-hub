{
  "name": "@nba-analytics-hub/testing",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nba-analytics-hub/types": "0.0.1"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "dependsOn": ["@nba-analytics-hub/types:build"],
        "options": {
          "tsConfig": "packages/testing/tsconfig.lib.json",
          "main": "packages/testing/src/index.ts",
          "outputPath": "packages/testing/dist",
          "format": ["esm"]
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": ["packages/testing/**/*.ts", "packages/testing/**/*.tsx"]
        }
      },
      "test": {
        "executor": "nx:noop",
        "options": {}
      }
    }
  }
}
