{
  "name": "@nba-analytics-hub/data-access",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nba-analytics-hub/types": "0.0.1"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "dependsOn": ["@nba-analytics-hub/types:build"],
        "options": {
          "tsConfig": "packages/data-access/tsconfig.lib.json",
          "main": "packages/data-access/src/index.ts",
          "outputPath": "packages/data-access/dist",
          "format": ["esm"]
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": [
            "packages/data-access/**/*.ts",
            "packages/data-access/**/*.tsx"
          ]
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vitest run",
          "cwd": "packages/data-access"
        }
      }
    }
  }
}
