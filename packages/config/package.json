{
  "name": "@nba-analytics-hub/config",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "dependsOn": ["@nba-analytics-hub/types:build"],
        "options": {
          "tsConfig": "packages/config/tsconfig.lib.json",
          "main": "packages/config/src/index.ts",
          "outputPath": "packages/config/dist"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": ["packages/config/**/*.ts", "packages/config/**/*.tsx"]
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vitest run",
          "cwd": "packages/config"
        }
      }
    }
  }
}
